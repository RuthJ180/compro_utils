
# Directory for available sites in apache (if applicable)
APACHE_SA='/etc/apache2/sites-available'

# Your bitbucket password (Be sure to set permissions on this file)
BITBUCKET_PASS="password"

# Your bitbucket username.
BITBUCKET_USER="username"

# The socket or IP:port for PHP as a fastcgi.
FASTCGI_PASS="127.0.0.1:9000"

# The TLD for the local dev environment, without the dot
LOCAL_EXT="local"

# A prefix to prepend to alias names and so forth.
LOCAL_ENV='dev-'

# Override the current USER where used (build instead of cashew)
USER_OVERRIDE=false

# Your MySQL root password.
MYSQL_PASS='password'

# Localhost IP that nginx should listen on.
NGINX_IP="127.0.0.2"

# Directory for available sites in nginx (if applicable)
NGINX_SA='/etc/nginx/sites-available'

# Directory for enabled sites in nginx (if applicable)
NGINX_SE='/etc/nginx/sites-enabled'

# Which webserver to use.
WEB_SERVER='apache'

# Remote host (for alias)
REMOTE_HOST=''
REMOTE_USER=''